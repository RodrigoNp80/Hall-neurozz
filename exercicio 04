  a = input('\33[31mDigite algo :\33[m ')
print('\33[34m O tipo primitivo desse valor é :\33[m',type(a))
print('\33[31mSó tem espaços ?\33[m',   a.isspace())
print('\33[31m É um número ? : \33[m',  a.isnumeric())
print('\33[31m É alfabético ? : \33[m', a.isalpha())
print('\33[31m É alfanúmerico ? \33[m ',a.isalnum())
print('\33[31m Está em MAISCÚLO :\33[m',a.isupper())
print('\33[31m Está em MINUSCÚLO :\33[m',a.islower())
print('\33[31m Está CAPITALIZADA :\33[m',a.istitle())
